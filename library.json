{
  "name": "W3bstreamClient",
  "description": "A client library for interacting with w3bstream",
  "repository": {
    "type": "git",
    "url": "https://github.com/iotexproject/arduino-sdk"
  },
  "version": "0.3.1",
  "authors": [
    {
      "name": "IoTeX",
      "email": "support@iotex.io",
      "url": "https://iotex.io/"
    }
  ],
  "platforms": [
    "espressif8266",
    "espressif32",
    "atmelsam"
  ],
  "frameworks": [
    "arduino"
  ],
  "build": {
    "flags": [
      "-I src"
    ],
    "srcFilter": [
    ],
    "libCompatMode": "strict"
  },
  "lib_ldf_mode": "deep+",
  "export": {
    "include": [
      "src/*",
      "src/include/*",
      "examples/*",
      "*.md",
      "*.json",
      "*.properties",
      "LICENSE",
      "*.ini",
      "keywords.txt"
    ],
    "exclude": [
      "doc/*",
      "tests/*",
      "tools/*",
      "**/*.txt"
    ]
  }
}